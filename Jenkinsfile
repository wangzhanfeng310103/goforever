    agent any 
    
    stages {
        stage('Build') {            
            steps {                
                echo 'Building'            
            }        
        }        
        stage('Test') {            
            steps {                
                echo 'Testing'
                echo 'Test end'            
            }        
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
            }   
        }    
    }
}

